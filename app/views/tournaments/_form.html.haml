= form_for @tournament do |f|
  = f.error_messages
  %p
    = f.label :name
    %br
    = f.text_field :name
  %p
    = f.label :description
    %br
    = f.text_area :description
  %p
    = f.label :user
    %br
    = f.text_field :user
  %p
    = f.label :state
    %br
    = f.text_field :state
  %p
    = f.label :handle
    %br
    = f.text_field :handle
  %p
    = f.label :settings
    %br
    = f.text_area :settings
  %p
    = f.submit
