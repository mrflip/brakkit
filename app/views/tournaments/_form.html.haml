= form_for @tournament do |f|
  = f.error_messages
  %p
    = f.label :name
    %br
    = f.text_field :name
  %p
    = f.label :description
    %br
    = f.text_area :description
  %p
    = f.label :user_id
    %br
    = f.number_field :user_id
  %p
    = f.label :state
    %br
    = f.text_field :state
  %p
    = f.submit
