- title "Bracket"
- centered_layout true
- skip_sidebar    true

%h2 Info

%pre
  == size: #{@bracket.size}

  
.pools{ :id => 'pools_1' }
  %h2 pools
  - @bracket.pools.each do |pool|
    = render pool

.wings
  - @bracket.wings[0..0].each do |wing|
    = render wing

%hr.clear/
%p
  = link_to "Edit", edit_bracket_path(@bracket)
  |
  = link_to "Destroy", @bracket, :confirm => 'Are you sure?', :method => :delete
  |
  = link_to "View All", brackets_path
